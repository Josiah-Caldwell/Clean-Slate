{
  "name": "Clean Slate",
  "description": "Creating a safer internet for minors!",
  "version": "0.5",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://teddit.net/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16x16_Clean_Slate_Icon.png"
    }
  }
}	